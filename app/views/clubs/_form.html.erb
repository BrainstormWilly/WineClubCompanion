<%= form_for club do |f| %>
  <div class="panel-body">
    <div class="form-group">
      <%= f.label :name %>
      <%= f.text_field :name, class: "form-control" %>
    </div>
    <div class="form-group">
      <%= f.label :description %>
      <%= f.text_area :description, class: "form-control" %>
    </div>
  </div>
  <div class="panel-footer">
    <%= f.submit "Submit", class: "btn btn-primary" %>
  </div>
<% end %>
